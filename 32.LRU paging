#include <stdio.h>

int main() {
    int n, m, i, j, pageFaults = 0, least, flag;
    printf("Enter number of pages: ");
    scanf("%d", &n);
    int pages[n];
    printf("Enter the page reference sequence: ");
    for(i = 0; i < n; i++) scanf("%d", &pages[i]);

    printf("Enter number of frames: ");
    scanf("%d", &m);
    int frames[m], time[m];
    for(i = 0; i < m; i++) { frames[i] = -1; time[i] = 0; }

    for(i = 0; i < n; i++) {
        flag = 0;
        for(j = 0; j < m; j++) {
            if(frames[j] == pages[i]) {
                flag = 1; 
                time[j] = i; // update recent use
                break;
            }
        }
        if(flag == 0) { // page fault
            int min = 0;
            for(j = 1; j < m; j++)
                if(time[j] < time[min]) min = j;
            frames[min] = pages[i];
            time[min] = i;
            pageFaults++;
        }

        printf("Frames after accessing page %d: ", pages[i]);
        for(j = 0; j < m; j++) printf("%d ", frames[j]);
        printf("\n");
    }
    printf("Total Page Faults: %d\n", pageFaults);
    return 0;
}
